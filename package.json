{
  "name": "daslang-grammar",
  "displayName": "Daslang Grammar",
  "description": "A syntax highlighting for Daslang",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "daslang",
      "aliases": ["Daslang", "daslang"],
      "extensions": [".das"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "daslang",
      "scopeName": "source.daslang",
      "path": "./syntaxes/daslang.tmLanguage.json"
    }]
  }
}
